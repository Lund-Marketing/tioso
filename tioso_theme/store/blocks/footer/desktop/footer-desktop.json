{
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "flex-layout.col#col-1",
      "flex-layout.col#col-2",
      "flex-layout.col#col-3"
    ],
    "props": {
      "blockClass": "footer-desktop",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "flex-layout.col#col-1":{
    "children":[
      "flex-layout.row#logo-tioso",
      "flex-layout.row#contacto-footer"
    ]
  },
  "flex-layout.row#logo-tioso":{
    "children":[
      "logo#tioso-footer"
    ],
    "props":{
      "preventHorizontalStretch": true,
      "blockClass": "logo-tioso-footer"
    }
  },
  "logo#tioso-footer":{
    "props":{
      "url": "assets/logo/logo-tioso.png",
      "width": 120
    }
  },
  "flex-layout.row#contacto-footer":{
    "children":[
      "rich-text#contacto-footer"
    ]
  },
  "rich-text#contacto-footer":{
    "props":{
      "text": "MONSEÑOR BUFANO 3079\nSan Justo, Buenos Aires\n\nTel: (011) 2150-0122\ncrm@tiosohogar.com"
    }
  },
  "flex-layout.col#col-2":{
    "children":[
      "flex-layout.row#categorias-footer"
    ],
    "props":{
      "blockClass": "categorias-footer"
    }
  },
  "flex-layout.row#categorias-footer":{
    "children":[
      "vtex.menu@2.x:menu#menu-footer"
    ]
  },
  "vtex.menu@2.x:menu#menu-footer":{
    "props":{
      "orientation": "vertical",
      "blockClass": "menu-footer-desktop"
    },
    "children":[
      "menu-item#cocina",
      "menu-item#baño",
      "menu-item#pinturas",
      "menu-item#muebles",
      "menu-item#pisos-rev",
      "menu-item#bombas-tanques",
      "menu-item#hornos",
      "menu-item#herramientas"
    ]
  },
  "menu-item#cocina": {
    "props": {
        "id": "menu-item-cocina",
        "type": "custom",
        "iconId": null,
        "highlight": false,
        "itemProps": {
            "type": "internal",
            "href": "/cocina",
            "noFollow": false,
            "tagTitle": "Cocina",
            "text": "Cocina"
        },
        "blockClass":"footerLinkItem"
    }
  },
